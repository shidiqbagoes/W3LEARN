<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.w3learnteam.w3learn">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FlownDraw"></activity>
        <activity
            android:name=".learn.CSub_Item"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustNothing" />
        <activity
            android:name=".learn.HSub_Item"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustNothing" />
        <activity
            android:name=".home_drawer"
            android:screenOrientation="portrait" />
        <activity
            android:name=".about.About"
            android:screenOrientation="portrait" />
        <activity
            android:name=".learn.Result"
            android:screenOrientation="portrait" />
        <activity
            android:name=".splashscreen.SplashScreen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".quiz.QuizRules"
            android:screenOrientation="portrait" />
        <activity
            android:name=".quiz.QuizTime"
            android:screenOrientation="portrait" />
        <activity
            android:name=".quiz.Quiz_Result"
            android:screenOrientation="portrait" />
        <activity
            android:name=".learn.HTML_Learn"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustNothing" />
        <activity
            android:name=".learn.CSS_Learn"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustNothing" />
        <activity
            android:name=".learn.List_Code"
            android:screenOrientation="portrait" />
    </application>

</manifest>